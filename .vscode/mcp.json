{
  "servers": {
    "imas": {
	  // uv run imas-mcp --host 127.0.0.1 --port 8080
      "url": "https://imas-dd.iter.org/mcp",
      "type": "http"
    },
    "imas-debug": {
      "type": "http",
      "url": "http://127.0.0.1:8080/mcp"
    },
    "imas-ddv3": {
      "command": "uv",
      "args": ["run", "imas-mcp", "--transport", "stdio"],
      "env": {
        "IMAS_DD_VERSION": "3.42.2"
      },
      "type": "stdio"
    },
    "imas-ddv4": {
      "command": "uv",
      "args": ["run", "imas-mcp", "--transport", "stdio"],
      "env": {
        "IMAS_DD_VERSION": "4.0.0"
      },
      "type": "stdio"
    },
    "minds-db": {
      "url": "http://127.0.0.1:47334/mcp/sse",
      "type": "http"
    }
  },
  "inputs": []
}
