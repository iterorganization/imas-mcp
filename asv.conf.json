{
    "version": 1,
    "project": "imas-mcp",
    "repo": ".",
    "branches": ["main", "HEAD"],
    "dvcs": "git",
    "environment_type": "virtualenv",
    "pythons": ["3.12"],
    "build_command": [
        "python -m pip install build",
        "python -c \"import os,subprocess,sys; os.environ['IDS_FILTER']='core_profiles,equilibrium'; os.environ['IMAS_MCP_EMBEDDING_MODEL']='all-MiniLM-L6-v2'; outdir=r'{build_cache_dir}'.replace(chr(92), '/'); builddir=r'{build_dir}'.replace(chr(92), '/'); subprocess.run([sys.executable, '-m', 'build', '--outdir', outdir, builddir], check=True)\""
    ],
    "install_command": [
        "in-dir={env_dir} python -m pip install {wheel_file}[transformers]"
    ],
    "uninstall_command": [
        "return-code=any python -m pip uninstall -y {project}"
    ],
    "matrix": {
        "env_nobuild": {
            "IMAS_MCP_EMBEDDING_MODEL": ["all-MiniLM-L6-v2"],
            "HF_HUB_DISABLE_SYMLINKS_WARNING": ["1"]
        }
    },
    "benchmark_dir": "benchmarks",
    "env_dir": ".asv/env",
    "results_dir": ".asv/results",
    "html_dir": ".asv/html",
    "build_cache_size": 8,
    "regressions_first_commits": {},
    "regressions_thresholds": {
        "regress": 1.5,
        "improve": 0.8
    }
}
